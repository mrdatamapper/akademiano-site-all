<!doctype html>
<html lang={{ html.lang|default("ru") }}>
<head>
    <meta charset="utf-8"/>
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset_img("/assets/img/apple-icon.png", default, true) }}">
    <link rel="icon" type="image/png" href="{{ asset_img("/assets/img/favicon.png", default, true) }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>

    <title>{{ pageTitle | default('Datamapper Site') }}</title>
    {% if pageDescription %}
        <meta name="description" content="{{ pageDescription }}"/>
    {% endif %}

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport'/>

    <!--     Fonts and icons     -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons"/>
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700"/>

    {{ asset_css([
    "font-awesome/css/font-awesome.min.css",
    ], default, true) }}

    <!-- CSS Files -->
    {{ asset_css([
    "bootstrap/dist/css/bootstrap.min.css",
    "assets/css/material-kit.css",
    "assets/css/material-dashboard.css",
    "assets/css/material-dashboard-kit-fix.css",
    ], default, true) }}

</head>

{% block body %}
    <body>


    </body>
{% endblock body %}

<!--   Core JS Files   -->
{{ asset_js([
"jquery/dist/jquery.min.js",
"bootstrap/dist/js/bootstrap.min.js",
"assets/js/material.min.js"
], default, true) }}


<!--  Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
{{ asset_js("assets/js/nouislider.min.js", default, true) }}

<!--  Plugin for the Datepicker, full documentation here: http://www.eyecon.ro/bootstrap-datepicker/ -->
{{ asset_js("assets/js/bootstrap-datepicker.js", default, true) }}

<!-- Control Center for Material Kit: activating the ripples, parallax effects, scripts from the example pages etc -->
{{ asset_js([
"assets/js/material-kit.js",
"assets/js/material-dashboard.js"
], default, true) }}

{% block jsAdditional %}

{% endblock jsAdditional %}

</html>
